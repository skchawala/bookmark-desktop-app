    "url": "https://github.com/skchawala/bookmark-desktop-app"
  },
  "homepage": "https://github.com/skchawala/bookmark-desktop-app/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/skchawala/bookmark-desktop-app/issues"
  },
  "author": "Satish Kumar",
  "license": "ISC",
  "devDependencies": {
    "electron": "9.0.0",
    "electron-builder": "22.10.5"
  },
  "dependencies": {
    "electron-window-state": "5.0.3",
    "nodemon": "2.0.7"
  },
  "build": {
    "appId": "com.bookmarkDesktopApp.app",
    "copyright": "Copyright bookmarkDesktopApp",
    "mac": {
      "category": "public.app-category.productivity",
       "target": "dmg"
    },
    "win": {
      "target": "zip"
    },
    "linux": {
      "category": "Utility"
